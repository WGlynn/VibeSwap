{
  "name": "vibeswap-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/update-build-version.js",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "postbuild": "node scripts/validate-build.js",
    "build:gh": "vite build --base=/vibeswap/",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "verify": "node scripts/verify-deployment.js",
    "deploy": "npm run build:production && npx vercel --prod",
    "deploy:verify": "npm run deploy && npm run verify",
    "persistance": "node scripts/persistance.js && npm run build",
    "deploy:gh": "npm run build:gh && gh-pages -d dist"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@reown/appkit": "^1.8.18",
    "@reown/appkit-adapter-ethers": "^1.8.18",
    "@simplewebauthn/browser": "^13.2.2",
    "ethers": "^6.16.0",
    "framer-motion": "^10.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "terser": "^5.46.0",
    "vite": "^5.0.0"
  }
}
