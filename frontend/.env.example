# ============================================
# FEATURE FLAGS
# ============================================
# Set to 'true' to enable production mode (disables mock data)
VITE_PRODUCTION_MODE=false

# Set to 'true' to enable mainnet chains
VITE_ENABLE_MAINNET=false

# Set to 'true' to disable testnet chains in production
VITE_DISABLE_TESTNETS=false

# ============================================
# BACKEND API
# ============================================
# Backend API base URL (defaults to /api for same-origin proxy)
VITE_API_URL=/api

# WebSocket URL (defaults to auto-detect from window.location)
VITE_WS_URL=

# ============================================
# API ENDPOINTS (Optional)
# ============================================
# If using external APIs for price feeds, analytics, etc.
VITE_PRICE_API_URL=
